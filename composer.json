{
  "name": "maksimru/laravel-fluent-logger",
  "description": "fluent logger for laravel and lumen with stackdriver support",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": [
    "laravel",
    "log",
    "logging",
    "fluent",
    "lumen",
    "stackdriver"
  ],
  "authors": [
    {
      "name": "Maksim Martianov",
      "email": "maxmtmn@gmail.com"
    }
  ],
  "require": {
    "php": ">=7",
    "fluent/logger": "~1.0",
    "illuminate/log": "~5.6",
    "illuminate/support": "~5.6",
    "illuminate/config": "~5.6",
    "illuminate/contracts": "~5.6",
    "illuminate/container": "~5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "satooshi/php-coveralls": "*"
  },
  "autoload": {
    "psr-4": {
      "Ytake\\LaravelFluent\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  }
}
